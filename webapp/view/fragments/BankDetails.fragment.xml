<core:FragmentDefinition xmlns:core="sap.ui.core"
    xmlns="sap.m">


    <Panel id="pAccountDetails" expandable="false" headerText="AccountDetails" width="auto" class="sapUiResponsiveMargin">
        <content>
            <HBox id="hBox4">

                <Avatar id="aUserProfile" src="" displaySize="XL" showBorder="true" press="onPress"/>

                <VBox id="vBox1" class="sapUiSmallMargin sapUiLargeMarginBegin">
                    <Title text="{i18n>accDetails}" id="acdetails" level="H1"></Title>

                    <HBox id="hBox1">
                        <Label text="{i18n>accNum}" id="bankAccNum" class="sapUiSmallMarginEnd"></Label>
                        <Text text="247865******" id="bankAccVal"></Text>
                    </HBox>

                    <HBox id="hBox2">
                        <Label text="{i18n>accHName}" id="lblAccHolderNameID" class="sapUiSmallMarginEnd"></Label>
                        <Text text="Neha Kumari" id="idAccHolderNameValue"></Text>
                    </HBox>

                    <HBox id="hBox3">
                        <Label text="{i18n>iFSCode}" id="lblIFSCId" class="sapUiSmallMarginEnd"></Label>
                        <Text text="000000" id="idIFSCValue"></Text>
                    </HBox>

                    <Button id="openBankDetails" type="Neutral" text="{i18n>findMoreBtn}" press=".onOpenBankDetails" />


                    <Button id="OpenDemoBtn" type="Neutral" text="click me" press=".onOpenDemoBtn" />


                </VBox>
            </HBox>

        </content>
    </Panel>





</core:FragmentDefinition>